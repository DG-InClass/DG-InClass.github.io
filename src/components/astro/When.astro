---
import { Aside } from '@astrojs/starlight/components';

/* Usage:

<When planned="Feb 1" actual="Feb 4" />

*/
interface Props {
    planned: string,
    actual?: string
}

const { planned, actual = "TBD" }:Props = Astro.props;
---

<Aside type="note" title="Scheduled">
    Planned for {planned} &ndash; Delivered on {actual}
</Aside>
